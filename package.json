{
	"name": "cra-starter",
	"version": "0.1.0",
	"scripts": {
		"build": "react-scripts build",
		"check-types": "tsc --noEmit --noUnusedLocals",
		"cypress:open": "cypress open",
		"dev": "react-scripts start",
		"generate": "npx generate-react-cli component",
		"fix:eslint": "npm run lint:eslint -- --fix",
		"lint": "npm-run-all lint:*",
		"lint:eslint": "eslint ./src --ext .js,.jsx,.ts,.tsx --cache",
		"test": "react-scripts test --setupFilesAfterEnv ./src/setupTests.ts",
		"storybook": "start-storybook -p 6006 -s public",
		"build-storybook": "build-storybook -s public",
		"prepare": "husky install"
	},
	"browserslist": {
		"production": [
			">0.2%",
			"not dead",
			"not op_mini all"
		],
		"development": [
			"last 1 chrome version",
			"last 1 firefox version",
			"last 1 safari version"
		]
	},
	"lint-staged": {
		"**/*.{js,jsx,ts,tsx}": [
			"yarn eslint ./src --ext .js,.jsx,.ts,.tsx --cache",
			"yarn prettier -w",
			"yarn test --watchAll=false --findRelatedTests --bail"
		]
	},
	"devDependencies": {
		"@storybook/addon-actions": "^6.4.19",
		"@storybook/addon-docs": "^6.4.19",
		"@storybook/addon-essentials": "^6.4.19",
		"@storybook/addon-links": "^6.4.19",
		"@storybook/builder-webpack5": "^6.4.19",
		"@storybook/manager-webpack5": "^6.4.19",
		"@storybook/node-logger": "^6.4.19",
		"@storybook/preset-create-react-app": "^4.0.1",
		"@storybook/react": "^6.4.19",
		"@typescript-eslint/eslint-plugin": "^4.33.0",
		"@typescript-eslint/parser": "^4.33.0",
		"eslint": "^7.32.0",
		"eslint-config-airbnb": "^19.0.2",
		"eslint-config-prettier": "8.5.0",
		"eslint-import-resolver-typescript": "^2.7.0",
		"eslint-plugin-cypress": "^2.12.1",
		"eslint-plugin-import": "^2.25.4",
		"eslint-plugin-jsx-a11y": "^6.5.1",
		"eslint-plugin-prettier": "4.0.0",
		"eslint-plugin-react": "^7.29.4",
		"eslint-plugin-react-hooks": "^4.2.0",
		"eslint-plugin-storybook": "^0.5.7",
		"husky": "^7.0.0",
		"lint-staged": "^12.3.5",
		"storybook-addon-styled-component-theme": "^2.0.0",
		"webpack": "5"
	},
	"dependencies": {
		"@hookform/resolvers": "^2.8.8",
		"@testing-library/jest-dom": "^5.16.3",
		"@testing-library/react": "^12.1.4",
		"@testing-library/react-hooks": "^7.0.2",
		"@testing-library/user-event": "^13.5.0",
		"@types/jest": "^27.4.1",
		"@types/node": "^17.0.23",
		"@types/react": "^17.0.43",
		"@types/react-dom": "^17.0.13",
		"@types/react-router-dom": "^5.3.2",
		"@types/react-test-renderer": "^17.0.1",
		"@types/styled-components": "^5.1.24",
		"axios": "^0.26.1",
		"cypress": "^9.5.1",
		"framer-motion": "^6.2.8",
		"i18next": "^21.6.14",
		"i18next-browser-languagedetector": "^6.1.4",
		"msw": "^0.39.2",
		"npm-run-all": "^4.1.5",
		"polished": "^4.1.4",
		"prettier": "2.6.1",
		"react": "^17.0.2",
		"react-dom": "^17.0.2",
		"react-focus-lock": "^2.8.1",
		"react-hook-form": "^7.28.1",
		"react-i18next": "^11.16.2",
		"react-popper": "^2.2.5",
		"react-router-dom": "^6.2.2",
		"react-scripts": "5.0.0",
		"react-test-renderer": "^17.0.2",
		"styled-components": "^5.3.0",
		"ts-jest": "^27.1.4",
		"typescript": "4.4.4",
		"web-vitals": "^2.1.3",
		"zod": "^3.14.3"
	}
}
